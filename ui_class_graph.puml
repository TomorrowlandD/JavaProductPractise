@startuml
' 抽象包代表类
package ui.panels {
  class 面板类
}

package ui.dialogs {
  class 对话框类
}

' Swing基类
class JPanel
class JDialog
class JFrame

' MainWindow
class MainWindow

' 继承关系
JPanel <|-- ui.panels.面板类
JDialog <|-- ui.dialogs.对话框类
JFrame <|-- MainWindow

' 组合/依赖关系
MainWindow *-- ui.panels.面板类
MainWindow ..> ui.dialogs.对话框类

@enduml